function _init()
 --carrega save 
	cartdata("vini_plantinhas_1")
 --inicia o mouse -----------------------------------------------------------------------------------------------------------------------------------------------------------------+
 poke(0x5f2d, 1)
 
 loaded_id = 1
 
 cu1 = 0
end

function _update()
	if btnp(⬅️) then
		if(loaded_id-1 >0)	loaded_id-=1
	end
	
	if btnp(➡️) then
			if(loaded_id+1 <64)	loaded_id+=1
	end

end

function _draw()
 cls()
 local draw_y = 1
	draw_y = show_data_component(0,1,draw_y,126)
	if dget(loaded_id) != 0 then
	 draw_y =	show_data_component(loaded_id,1,draw_y,126)
	end

end

