--get specific data
function get_data(id)
	local d = {
		id      = id,
	 value   = 0,
		hex     = 0,
		title   = "",
		sprite  = 0,
		spr_w   = 1,
		spr_v   = 1,
		data_v  = {},
		data_n  = {},
		add_data = function(self,new_data,data_name)
			add(self.data_v,new_data)
			add(self.data_n,data_name)
		end,
 }
 
 d.value = dget(d.id)
	d.hex   = tostr(d.value,true)
 
 if id == 0 then
  d.title = "game_vars"
  d.sprite= 1
		d.spr_w = 2
		d.spr_v = 2
		
		d:add_data((d.value >>  4) & 0xfff,	"\fasaldo")
	 d:add_data((d.value <<  7) & 0x7ff,	"\feano")
	 d:add_data((d.value << 11) & 0xf  ,	"\f3mes")
  d:add_data((d.value << 16) & 0x1f	,	"\f4dia")
  
  d:add_data(999	,	"\f4cu1")

 elseif id == 1 then
 	d.title = "regador"
  d.sprite= 3
		d.spr_w = 2
		d.spr_v = 2
		
		d:add_data((d.value >>> 7) & 0x7f,	"\f8pos.x")
	 d:add_data((d.value >>> 0) & 0x7f,	"\fcpos.y")
 end
 
 
 return d
 
end
