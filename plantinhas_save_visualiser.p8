--data interface component
function show_data_component(data_id,x,y,w)
	--[[
	data_format = 
 {
		id      = id
	 value   = dget(id)
		hex     = tostr(value,true)
		title   = ""
		sprite  = 0
		spr_w   = 1
		spr_v   = 1
		data    = {}
 }
	]]

	local rd =	get_data(data_id)

	
	local aux_x = x
	local aux_y = y
	local aux_w = w
	local aux_h = 24
	
	--label area ---------------------------
	aux_y	+= 2
	--label text
	local label_str = ""
	label_str = label_str..rd.title..              "\f1|\f6"
	label_str = label_str.."\fdid:\fb"  ..rd.id.. "\f1|\f6"
	label_str = label_str.."\fdhex:\f9" ..rd.hex
	
	print(label_str,x+2,aux_y,6)

	--label line
	aux_y += 6

	line(x,aux_y,x+w,aux_y,5)
	aux_y += 2
	
	--sprite area ---------------------------------
	--sprite square]
	local spr_square = (rd.spr_w*8)+3
	rect(x+2,aux_y,x+2+spr_square,aux_y+spr_square,5)

	
	spr(rd.sprite,x+3,aux_y+3,rd.spr_w,rd.spr_v)
	-- datas ==============
	local max_str_len = get_longest_string_size(rd.data_n)
	local aux_str_y = aux_y

	for i=1, #rd.data_v do
		local aux_str = max_str_len-#rd.data_n[i]
		local aux_n   = rd.data_n[i].."\*"..aux_str.." "
  print(aux_n..':'..rd.data_v[i],x+spr_square+6,aux_str_y)
  aux_str_y+=6
	end

	local spr_y = aux_y+ spr_square
	
	if(aux_str_y<spr_y)aux_str_y = spr_y +2
	
	aux_y = aux_str_y

	rect(x,y,x+w,aux_y,1)
	
	return aux_y+2

end


