--utilitarios
function get_longest_string_size(tbl)
 local longest = ""
 for i=1,#tbl do
  if #tbl[i] > #longest then
   longest = tbl[i]
  end
 end
 return #longest
end

function range(val,⬅️,➡️)
	if(val >= ⬅️ and val <= ➡️)return true
end
