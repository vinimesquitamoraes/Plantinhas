--get specific data
function get_data(id)
	local d = {
		id      = id,
	 value   = 0,
		hex     = 0,
		title   = "",
		sprite  = 0,
		spr_w   = 1,
		spr_v   = 1,
		data_v  = {},
		data_n  = {},
		add_data = function(self,new_data,data_name)
			add(self.data_v,new_data)
			add(self.data_n,data_name)
		end,
 }
 
 d.value = dget(d.id)
	d.hex   = tostr(d.value,true)
 
 if id == 0 then
  d.title = "game_vars"
  d.sprite= 166
		d.spr_w = 2
		d.spr_v = 2
		
		d:add_data((d.value >>  4) & 0xfff,	"\fasaldo")
	 d:add_data((d.value <<  7) & 0x7ff,	"\feano")
	 d:add_data((d.value << 11) & 0xf  ,	"\f3mes")
  d:add_data((d.value << 16) & 0x1f	,	"\f4dia")
 
 elseif range(id,1,63) then

 	d.title = "item"
 
	 local tipo = (d.value <<   5) & 0x1f
		if(tipo == 0)then
			tipo = tipo.." inv slot"
			d.sprite  = 2
			d.spr_w   = 2
			d.spr_v   = 2
		end
		if(tipo == 1)then
			tipo = tipo.." fertilizer"
			d.sprite  = 8
			d.spr_w   = 2
			d.spr_v   = 2
		end
		if(tipo == 2)tipo = tipo.." pesticide"
		if(tipo == 3)tipo = tipo.." basket"
		
		if(tipo == 4)tipo = tipo.." flowerpot_1"
		if(tipo == 5)tipo = tipo.." flowerpot_2"
		if(tipo == 6)tipo = tipo.." flowerpot_3"
		if(tipo == 7)tipo = tipo.." flowerpot_4"

		if(tipo == 8)tipo = tipo.." tomato"
		if(tipo == 9)tipo = tipo.." bear_pawn"
		if(tipo ==10)tipo = tipo.." pumpkin"
		if(tipo ==11)tipo = tipo.." pegaxi"
		if(tipo ==12)tipo = tipo.." sunflower"
		if(tipo ==13)tipo = tipo.." sword"
		if(tipo ==14)tipo = tipo.." rose"
		if(tipo ==15)tipo = tipo.." dandelion"
	
		if(tipo ==16)then
			tipo = tipo.." watering_can"
			d.sprite  = 204
			d.spr_w   = 2
			d.spr_v   = 2
		end
		if(tipo ==17)tipo = tipo.." shovel"
		
		d:add_data(tipo,	"\f9tipo")
 
 	local onde =  (d.value >>> 14) & 0x03
		if(onde == 1)onde = "\f3jardim"
		if(onde == 2)onde = "\f4inventario"
		if(onde == 3)onde = "\featalho"

 	if(onde != 0)	d:add_data( onde,	"\f4onde")
		
		d:add_data((d.value >>> 7) & 0x7f,	"\f8pos.x")
	 d:add_data((d.value >>> 0) & 0x7f,	"\fcpos.y")

 end
 
 
 return d
 
end
